{
	"manifest_version": 2,
	"name": "DoTory",
	"description": "This extension manages multiple histories.",
	"version": "1.0",
	"icons":{
		"48" : "images/acorn-48.png"
	},

	"browser_action": {
		"default_icon": "images/acorn-19.png",
		"default_popup": "source/dotory.html"
	},
	
	"permissions":[
		"identity",
		"https://accounts.google.com/*",
    	"https://www.googleapis.com/*",
		"activeTab",
		"tabs",
 		"http://*/*", "https://*/*" 		
 	],
 	
 	"background":{
		"persistent":true,
		"page":"source/getURL.html"
	},
		
	"key" : "",
	"minimum_chrome_version": "29",
	"oauth2": {
		"client_id": "26990809457-u5h4cfuo476sl3rva69crhn45n7vos1r.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/plus.login"
		]
	}
}
