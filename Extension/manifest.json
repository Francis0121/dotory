{
  "manifest_version": 2,

  "name": "DoTory",
  "description": "This extension manages multiple histories.",
  "version": "1.7",
  "icons":{
	"48" : "images/acorn-48.png"
  },

  "browser_action": {
    "default_icon": "images/acorn-19.png",
    "default_popup": "source/dotory.html"
  },
  "permissions":[
  	"activeTab",
  	"tabs",
  	"http://*/*", "https://*/*"
  ],
  "background":{
  	"background_page":"getURL.html"
  }
  
  
}
